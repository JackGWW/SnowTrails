# Cloudflare R2 Configuration for GPX Upload
# Copy this file to .env and fill in your values

# Your Cloudflare account ID (found in R2 dashboard)
R2_ACCOUNT_ID=your_account_id_here

# R2 API token credentials (generate in Cloudflare dashboard)
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here

# R2 bucket name where recordings will be uploaded
R2_BUCKET_NAME=snowtrails-recordings
