{
  "expo": {
    "name": "SnowTrails",
    "slug": "SnowTrails",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1679F3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jgwwilliams.snowtrails",
      "buildNumber": "1.4.0",
      "config": {
        "googleMapsApiKey": "AIzaSyDwiFWcgGSy2wKw3oZgoUGrS25fYKhHF4A"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the users location to show their current location on a trail map"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_foreground.png",
        "backgroundColor": "#1679F3"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDyxf__edqt0S_bxIHi193JraImCgI02jA"
        }
      },
      "icon": "./assets/icon.png",
      "package": "com.jgwwilliams.SnowTrails",
      "versionCode": 21,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "snowtrails",
          "project": "snowtrails",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f342101d-f80c-4515-a297-523366dad004"
