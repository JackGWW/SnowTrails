{
  "expo": {
    "name": "SnowTrails",
    "slug": "SnowTrails",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1679F3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jgwwilliams.snowtrails",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon512.png",
        "backgroundColor": "#1679F3"
      },
      "icon": "./assets/icon.png",
      "package": "com.jgwwilliams.SnowTrails",
      "versionCode": 4,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"] 

    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
